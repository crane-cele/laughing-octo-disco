sys = read_in {
    int_file = "INTDUMP",
    nel = 13,
    ms = 1,
    CAS = {9, 12},
}

hilbert_space {
    sys = sys,
    hilbert = {
        ncycles = 10000000,
        rng_seed = 7,
    },
}
fciqmc {
    sys = sys,
    qmc = {
        tau = 0.005,
        rng_seed = 7,
        init_pop = 10,
        mc_cycles = 10,
        nreports = 200,
        target_population = 77000,
        real_amplitudes = true,
        real_amplitude_force_32 = true,
        spawn_cutoff = 0.01,
        state_size = -500,
        spawned_state_size = -100,
    },
}
