nactive = 8

system = {
    int_file = "FCIDUMP",
    nel = 24,
    ms = 0,
    sym = 0,
    CAS = {nactive, 8},
    complex = true,
}

sys = read_in(system)

hdf5_file = write_read_in_system {
    sys = sys,
    }

sys:free()
system.int_file = hdf5_file
system.CAS = nil
system.nel = nactive

sys = read_in(system)

fciqmc {
    sys = sys,
    qmc = {
        tau = 1e-2,
        rng_seed = 23,
        init_pop = 10,
        mc_cycles = 20,
        real_amplitudes = true,
        nreports = 90,
        target_population = 7500,
        state_size = 10000,
        spawned_state_size = 5000,
    },
}
